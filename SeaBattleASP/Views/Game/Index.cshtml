@model SeaBattleASP.Models.MapModel

@{
    ViewData["Title"] = "Game Page";
}

<table class="table">
    @foreach (Player p in Model.Players)
    {
        <tr>
            <td>@p.Id</td>
            <td>@p.Name</td>
            <td>
               <button onclick="return createGame(@p.Id)">PLAY</button>
            </td>
        </tr>
    }
</table>